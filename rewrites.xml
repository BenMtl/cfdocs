<?xml version="1.0" encoding="utf-8"?>
<urlrewrite>
    <rule>
        <from>^/try/([^./]+)/([0-9]+)/?$</from>
        <to>/trycf.cfm?name=$1&amp;index=$2</to>
    </rule>
    <rule>
        <from>^/([^./]*[A-Z][^./]+)$</from>
        <to>/ucase.cfm?uri=$1</to>
    </rule>
    <rule>
        <from>^/([^./]+)$</from>
        <to>/doc.cfm?name=$1</to>
    </rule>
    <rule>
        <from>^/tag-summary$</from>
        <to type="redirect">/tags</to>
    </rule>
    <rule>
        <from>^/t$</from>
        <to type="redirect">/tags</to>
    </rule>
    <rule>
        <from>^/f$</from>
        <to type="redirect">/functions</to>
    </rule>
    <rule>
        <from>^/application.cfc$</from>
        <to type="redirect">/application-cfc</to>
    </rule>
    <rule>
        <from>^/try/([^./]+)/?$</from>
        <to>/trycf.cfm?name=$1</to>
    </rule>
    <rule>
        <from>^/assets/v-[A-Za-z0-9-]+/([a-zA-Z0-9/._-]+)$</from>
        <to>/assets/$1</to>
    </rule>

</urlrewrite>
